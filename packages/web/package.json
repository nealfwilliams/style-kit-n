{
  "name": "@style-kit-n/web",
  "version": "0.5.0",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "main": "dist/index.js",
  "module": "dist/web.esm.js",
  "size-limit": [
    {
      "path": "dist/web.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/web.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "@style-kit-n/core": "^0.5.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.4",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "husky": "^8.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "size-limit": "^8.2.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "jest": {
    "testEnvironment": "jsdom"
  }
}
